    {
      "name": "Keyword \"new\"",
      "scope": [
        "keyword.control.new",
        "keyword.operator.new",
        "keyword.other.new"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Storage",
